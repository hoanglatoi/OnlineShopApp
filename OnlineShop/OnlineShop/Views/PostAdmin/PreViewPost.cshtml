@{
    ViewData["Title"] = "PreView Post For Admin";
    Layout = "_AdminLayout";
    var id = ViewData["id"];
    var htmlCode = ViewData["htmlcode"];
}
<head>
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
</head>
<html>
    <body>
    <div style="width: 1000px">
        @Html.Raw(htmlCode)
    </div>
    <div style="margin-top: 20px">
        <form>
            <a asp-action="IndexPost" asp-route-id="@id" class="btn btn-primary">Trở Về Trang Danh Sách Bài Viết</a>
        </form>
    </div>
    </body>
</html>